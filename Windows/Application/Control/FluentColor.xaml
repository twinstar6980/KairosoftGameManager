<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	>
	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Light">
			<Color x:Key="FluentColor.Verbosity">#8A8A8A</Color>
			<Color x:Key="FluentColor.VerbosityContainer">#00000000</Color>
			<Color x:Key="FluentColor.Information">#0078D4</Color>
			<Color x:Key="FluentColor.InformationContainer">#DCECF9</Color>
			<Color x:Key="FluentColor.Warning">#9D5D00</Color>
			<Color x:Key="FluentColor.WarningContainer">#FFF4CE</Color>
			<Color x:Key="FluentColor.Error">#C42B1C</Color>
			<Color x:Key="FluentColor.ErrorContainer">#FDE7E9</Color>
			<Color x:Key="FluentColor.Success">#0F7B0F</Color>
			<Color x:Key="FluentColor.SuccessContainer">#DFF6DD</Color>
			<Color x:Key="FluentColor.Input">#9E3AB0</Color>
			<Color x:Key="FluentColor.InputContainer">#F3E7F6</Color>
		</ResourceDictionary>
		<ResourceDictionary x:Key="Dark">
			<Color x:Key="FluentColor.Verbosity">#9D9D9D</Color>
			<Color x:Key="FluentColor.VerbosityContainer">#00000000</Color>
			<Color x:Key="FluentColor.Information">#4CC2FF</Color>
			<Color x:Key="FluentColor.InformationContainer">#34424D</Color>
			<Color x:Key="FluentColor.Warning">#FCE100</Color>
			<Color x:Key="FluentColor.WarningContainer">#433519</Color>
			<Color x:Key="FluentColor.Error">#FF99A4</Color>
			<Color x:Key="FluentColor.ErrorContainer">#442726</Color>
			<Color x:Key="FluentColor.Success">#6CCB5F</Color>
			<Color x:Key="FluentColor.SuccessContainer">#393D1B</Color>
			<Color x:Key="FluentColor.Input">#D88DE1</Color>
			<Color x:Key="FluentColor.InputContainer">#321D35</Color>
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
